;AAAA;AAGA,gBAAA,QAHA;AAIA,gBAAA,OAAA,CAAA,GAAA,CAAA,QAAA;AACA,gBAAA,KALA,GAKA,QAAA,CAAA,IAAA,CAAA,KALA;;AAMA,oBAAA,KAAA,EAAA;AACA;AACA,kBAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA,GAAA;;AACA,kBAAA,IAAA,CAAA,SAAA,EAAA,6EAAA,EAAA,SAAA,CAAA;AACA,iBAJA,MAIA;AACA,kBAAA,IAAA,CAAA,OAAA,EAAA,qBAAA,EAAA,OAAA,CAAA;AACA;;AAZA;AAAA;;AAAA;AAAA;AAAA;AAcA,gBAAA,KAdA,GAcA,YAAA,QAdA;;AAeA,oBAAA,KAAA,CAAA,MAAA,IAAA,GAAA,EAAA;AACA,kBAAA,IAAA,CAAA,OAAA,EAAA,KAAA,CAAA,IAAA,CAAA,OAAA,EAAA,OAAA,CAAA;AACA,iBAFA,MAEA;AACA,kBAAA,IAAA,CAAA,OAAA,EAAA,KAAA,CAAA,IAAA,CAAA,GAAA,CAAA,OAAA,EAAA,OAAA,CAAA;AACA;;AAnBA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAqBA;AAtBA,GAnBA;AA2CA,EAAA,OA3CA,qBA2CA;AACA,QAAA,KAAA,GAAA,IAAA,IAAA,EAAA;AACA,QAAA,EAAA,GAAA,KAAA,CAAA,OAAA,EAAA;AACA,QAAA,EAAA,GAAA,KAAA,CAAA,QAAA,KAAA,CAAA,CAHA,CAGA;;AACA,QAAA,OAAA,GAAA,KAAA,CAAA,WAAA,KAAA,EAAA;AACA,QAAA,OAAA,GAAA,KAAA,CAAA,WAAA,KAAA,GAAA;;AACA,QAAA,EAAA,GAAA,EAAA,EAAA;AACA,MAAA,EAAA,GAAA,MAAA,EAAA;AACA;;AACA,QAAA,EAAA,GAAA,EAAA,EAAA;AACA,MAAA,EAAA,GAAA,MAAA,EAAA;AACA;;AAEA,IAAA,KAAA,GAAA,OAAA,GAAA,GAAA,GAAA,EAAA,GAAA,GAAA,GAAA,EAAA;AACA,IAAA,QAAA,CAAA,aAAA,CAAA,gBAAA,EAAA,YAAA,CAAA,KAAA,EAAA,KAAA;AACA,IAAA,KAAA,GAAA,OAAA,GAAA,GAAA,GAAA,EAAA,GAAA,GAAA,GAAA,EAAA;AACA,IAAA,QAAA,CAAA,aAAA,CAAA,gBAAA,EAAA,YAAA,CAAA,KAAA,EAAA,KAAA;AACA;AA5DA,CAAA","sourcesContent":["<template>\r\n  <div id=\"register\">\r\n    <form @submit.prevent=\"registerUser\">\r\n      <div class=\"sex\"> \r\n        <p class=\"title\">Civilité</p>\r\n        <div class=\"input\">\r\n          <div class=\"inputSex\">\r\n            <input\r\n              type=\"radio\"\r\n              id=\"sexeH\"\r\n              name=\"sexe\"\r\n              value=\"H\"\r\n              v-model=\"register.civilite\"\r\n              required\r\n            />\r\n            <label for=\"sexeH\">Monsieur</label>\r\n          </div>\r\n          <div class=\"inputSex\">\r\n            <input\r\n              type=\"radio\"\r\n              id=\"sexeF\"\r\n              name=\"sexe\"\r\n              value=\"F\"\r\n              v-model=\"register.civilite\"\r\n              required\r\n            />\r\n            <label for=\"sexeF\">Madame</label>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div id=\"prenomEtNom\">\r\n        <div class=\"input\">\r\n          <p class=\"title\">Prénom</p>\r\n          <CustomInput type=\"text\" label=\"Prénom\" v-model=\"register.prenom\" icon=\"user\" required/>\r\n        </div>\r\n        <div class=\"input\">\r\n          <p class=\"title\">Nom</p>\r\n          <CustomInput type=\"text\" label=\"Nom\" v-model=\"register.nom\" icon=\"user\"/>\r\n        </div>\r\n      </div>\r\n      <div id=\"date\">\r\n        <p class=\"title\">Date de naissance</p>\r\n        <input\r\n        type=\"date\"\r\n        id=\"dateNaissance\"\r\n        class=\"date\"\r\n        v-model=\"register.dateNaissance\"\r\n        required\r\n        />\r\n      </div>\r\n      <div id=\"nomUser\">\r\n        <p class=\"title\">Nom d'utilisateur</p>\r\n        <CustomInput type=\"text\" label=\"Nom d'utilisateur\" v-model=\"register.nomUtilisateur\" icon=\"user\" required/>\r\n      </div>\r\n      <div id=\"email\">\r\n        <p class=\"title\">Email</p>\r\n        <CustomInput type=\"email\" label=\"Email\" v-model=\"register.email\" icon=\"at\" required />\r\n      </div>\r\n      <div id=\"password\">\r\n        <p class=\"title\">Mot de passe</p>\r\n         <CustomInput type=\"password\" label=\"Mot de passe\" v-model=\"register.password\" icon=\"lock\" required/>\r\n      </div>\r\n      <div id=\"bottom\">\r\n        <div id=\"lien\">\r\n          <p>Déjà un compte ? <router-link to=\"/login\">Se connecter</router-link></p>\r\n          <p>Je ne suis pas <a href=\"\" @click=\"this.$emit('retour', 0)\"> {{ this.statutUser }}</a></p>\r\n        </div>\r\n        <button type=\"submit\">\r\n            S'enregistrer\r\n          </button>\r\n      </div>     \r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport swal from \"sweetalert\";\r\nimport CustomInput from \"@/components/CustomInput\"\r\nexport default {\r\n  components: {\r\n    CustomInput\r\n  },\r\n  props: ['statutUser'],\r\n  data() {\r\n    return {\r\n      register: {\r\n        civilite: \"\",\r\n        prenom: \"\",\r\n        nom: \"\",\r\n        dateNaissance: \"\",\r\n        statut: \"\",\r\n        nomUtilisateur: \"\",\r\n        email: \"\",\r\n        password: \"\",\r\n      },\r\n    };\r\n  },\r\n  methods: {\r\n    async registerUser() {\r\n      try {\r\n        this.register.statut = this.statutUser;\r\n        let response = await this.$http.post(\"/user/register\", this.register);\r\n        console.log(response);\r\n        let token = response.data.token;\r\n        if (token) {\r\n          //localStorage.setItem(\"jwt\", token);  On ne veut pas que le membre soit directement connecté\r\n          this.$router.push(\"/\");\r\n          swal(\"Success\", \"Inscription Réussie \\n Veuillez attendre d'être validé par l'administrateur\", \"success\");\r\n        } else {\r\n          swal(\"Error\", \"Inscription échouée\", \"Error\");\r\n        }\r\n      } catch (err) {\r\n        let error = err.response;\r\n        if (error.status == 409) {\r\n          swal(\"Error\", error.data.message, \"error\");\r\n        } else {\r\n          swal(\"Error\", error.data.err.message, \"error\");\r\n        }\r\n      }\r\n    },\r\n  },\r\n  mounted() {\r\n    var today = new Date();\r\n    var dd = today.getDate();\r\n    var mm = today.getMonth()+1; //January is 0 so need to add 1 to make it 1!\r\n    var yyyyMax = today.getFullYear() - 15;\r\n    var yyyyMin = today.getFullYear() - 100;\r\n    if(dd<10){\r\n      dd='0'+dd\r\n    } \r\n    if(mm<10){\r\n    mm='0'+mm\r\n    } \r\n\r\n    today = yyyyMax+'-'+mm+'-'+dd;\r\n    document.querySelector(\"#dateNaissance\").setAttribute(\"max\", today);\r\n    today = yyyyMin+'-'+mm+'-'+dd;\r\n    document.querySelector(\"#dateNaissance\").setAttribute(\"min\", today);\r\n  },\r\n};\r\n</script>\r\n<style lang=\"scss\" scoped>\r\n$color: #26F191;\r\n#register {\r\n  user-select: none;\r\n  min-width: 400px;\r\n  width: 50vw;\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: center;\r\n  align-items: center;\r\n  form {\r\n    .title{\r\n      font-size: 2vh;\r\n      font-weight: 700;\r\n    }\r\n    .sex {\r\n      .input {\r\n        display: flex;\r\n        flex-flow: row nowrap;\r\n        justify-content: space-around;\r\n      }\r\n    }\r\n    #prenomEtNom {\r\n      display: flex;\r\n      flex-flow: row nowrap;\r\n      justify-content: space-between;\r\n      .input {\r\n        width: 45%;\r\n\r\n      }\r\n    }\r\n    .date {\r\n      min-width: 10vw;\r\n      \r\n      padding: 0.9vh 2.2rem 0.9vh 1.2rem;\r\n      outline: none;\r\n      border: 2px solid $color;\r\n      border-radius: 5px;\r\n      margin-bottom: 0.5rem;\r\n    }\r\n    #bottom {\r\n      display: flex;\r\n      flex-flow: row wrap;\r\n      justify-content: space-between;\r\n      align-items: center;\r\n      #lien {\r\n        margin-top: 2vh;\r\n        a {\r\n          color: $color;\r\n        }\r\n      }\r\n      button {\r\n        background-color: $color;\r\n        \r\n        font-size: 1.75vh;\r\n        border-radius: 5px;\r\n        border: none;\r\n        color: white;\r\n      }\r\n    }\r\n  }\r\n}\r\n</style>\r\n"],"sourceRoot":"src/components"}]}