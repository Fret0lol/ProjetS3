A;AACA,MAAA,IAAA,CAAA,SAAA,CAAA,MAAA,CAAA,eAAA;AACA,MAAA,IAAA,CAAA,SAAA,CAAA,GAAA,CAAA,YAAA;AACA,KAXA;AAYA,IAAA,MAZA,qBAYA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,KAAA,CAAA,SAAA;;AACA,gBAAA,MAFA,GAEA;AACA,kBAAA,EAAA,EAAA,KAAA,CAAA;AADA,iBAFA;AAAA;AAAA,uBAMA,KAAA,CAAA,KAAA,CAAA,GAAA,CAAA,mBAAA,EAAA,MAAA,CANA;;AAAA;AAOA,gBAAA,QAAA,CAAA,MAAA;;AAPA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAQA,KApBA;AAsBA,IAAA,QAtBA,sBAsBA;AACA,WAAA,UAAA,GAAA,IAAA;AACA,WAAA,MAAA,GAAA,QAAA,CAAA,aAAA,YAAA,KAAA,GAAA,wCAAA,SAAA;AACA,WAAA,SAAA;AACA,KA1BA;AA2BA,IAAA,aA3BA,2BA2BA;AACA,WAAA,UAAA,GAAA,KAAA;AACA,KA7BA;AA+BA,IAAA,UA/BA,wBA+BA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,MADA,GACA;AACA,kBAAA,EAAA,EAAA,MAAA,CAAA,MADA;AAEA,kBAAA,kBAAA,EAAA,QAAA,CAAA,aAAA,CAAA,iBAAA,EAAA;AAFA,iBADA;AAAA;AAAA,uBAKA,MAAA,CAAA,KAAA,CAAA,GAAA,CAAA,mBAAA,EAAA;AAAA,kBAAA,MAAA,EAAA;AAAA,iBAAA,CALA;;AAAA;AAMA,gBAAA,MAAA,CAAA,aAAA;;AACA,gBAAA,MAAA,CAAA,OAAA,CAAA,EAAA;;AAPA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAQA;AAvCA;AARA,CAAA","sourcesContent":["<template>\r\n    <div class=\"commentaire\"  :id=\"[this._id + '_commentaire']\" :class=\"(this.isDelete)?'commentaire-delete':'notdelete'\" >\r\n        <div class=\"commentaire_header\">\r\n            <div class=\"commentaire_header_info\">\r\n                <img src=\"../assets/Logo.png\" class=\"commentaire_header_info_photo\">\r\n                <div class=\"commentaire_header_info_content\">\r\n                    <h2 class=\"commentaire_header_info_content_name\"> {{ this.nomUtilisateur }} </h2>\r\n                    <p class=\" commentaire_header_info_content_date\"> {{ this.date }} </p>\r\n                </div>\r\n            </div>\r\n            <img src=\"../assets/menu.png\" class=\"commentaire_header_menu\" @click=\"this.openMenu\"  v-if=\"this.isDelete === false\">\r\n        </div>\r\n         <p class=\"commentaire_content commentaire-rep\"  v-if=\"!this.isMofidied\">\r\n            {{ this.reponse }}\r\n        </p>\r\n        <p class=\"commentaire_content commentaire-content\"  v-if=\"!this.isMofidied\">\r\n            {{ this.commentaire }}\r\n        </p>\r\n\r\n        <div class=\"modif-wrapper\" v-if=\"this.isMofidied\">\r\n             <textarea class=\"textarea-modif\"  v-model=\"this.comVal\"> </textarea>\r\n             <input type=\"submit\" value=\"valider\" @click=\"this.updatePost\">\r\n             <input type=\"submit\" value=\"annuler\" @click=\"this.closeModified\">\r\n        </div>\r\n       \r\n        <img src=\"../assets/arrow.png\" class=\"commentaire_rep\" v-if=\"this.isDelete === false\" v-on:click=\"$emit('repondre', commentaire)\"/>\r\n        <div class=\"menu menu-close\" :id=\"[this._id + '_menu']\">\r\n            <ul class=\"menu_list\">\r\n                <li class=\"menu_list_item\" v-on:click=\"$emit('repondre', commentaire)\" @click=\"this.closeMenu\" >RÃ©pondre</li>\r\n                <li class=\"menu_list_item\" v-if=\"this.login == this.nomUtilisateur\" @click=\"modified\">Modifier</li>\r\n                <li class=\"menu_list_item\"  v-if=\"this.login == this.nomUtilisateur\" @click=\"this.delete\">Supprimer</li>\r\n                <li class=\"menu_list_item\" v-on:click=\"$emit('signaler', idPost)\">Signaler</li>\r\n            </ul>\r\n            <img  alt=\"\" src=\"../assets/menu.png\" class=\"menu_close\" @click=\"this.closeMenu\">\r\n        </div>\r\n    </div>\r\n</template>\r\n<script>\r\nexport default {\r\n    props : [\"reponse\",\"isDelete\",\"idPost\" ,\"_id\",\"nomUtilisateur\",\"date\",\"imageProfil\",\"commentaire\",\"login\"],\r\n    data() {\r\n        return {\r\n            isMofidied : false,\r\n            comVal : \"\",\r\n        }\r\n    },\r\n    methods: {\r\n\r\n        openMenu(){\r\n            const menu = document.querySelector(`#${this._id}_menu`); \r\n            menu.classList.add('menu-expanded');\r\n            menu.classList.remove('menu-close');\r\n        },  \r\n        closeMenu(){\r\n            const menu = document.querySelector(`#${this._id}_menu`);\r\n            menu.classList.remove('menu-expanded');\r\n            menu.classList.add('menu-close');\r\n        },\r\n        async delete(){\r\n            this.closeMenu();\r\n            const params = {\r\n                id : this.idPost,\r\n                \r\n            }\r\n             await this.$http.put('/forum/deletePost',params);\r\n            location.reload();\r\n        },\r\n\r\n        modified(){\r\n            this.isMofidied = true;   \r\n            this.comVal = document.querySelector(`#${this._id}_commentaire .commentaire-content`).innerText; \r\n            this.closeMenu();        \r\n        },\r\n        closeModified(){\r\n            this.isMofidied = false;\r\n        },\r\n        \r\n        async updatePost(){\r\n            const params = {\r\n                id : this.idPost,\r\n                contenuCommentaire : document.querySelector('.textarea-modif').value\r\n            }\r\n            await this.$http.put('/forum/updatePost',{params}); \r\n            this.closeModified();\r\n            this.$router.go()\r\n        }\r\n       \r\n    }\r\n}\r\n</script>\r\n<style lang=\"scss\" scoped>\r\n\r\n\r\n\r\n\r\n.textarea-modif{\r\n    width: 100%;\r\n    height: 100%;\r\n    padding: 1em;\r\n}\r\n\r\n.commentaire{\r\n    position: relative;\r\n    font-size: 0.9em;\r\n    width: 100%;\r\n    max-width: 1000px;\r\n    border: solid 1px #B0B0B0;\r\n    background: white;\r\n    border-radius:5px;\r\n    padding: 1em 1em 4em 1em;\r\n    margin: 1.5em 0;\r\n    \r\n    &-rep{\r\n        font-style: italic;\r\n    }\r\n    &-delete{\r\n    background: rgb(252, 152, 152)  !important;\r\n    }\r\n    &_header{\r\n        display: flex;\r\n        flex-direction: row;\r\n        justify-content: space-between;\r\n        align-items: flex-start;\r\n        margin-bottom: 1em;\r\n        &_info{\r\n            display: flex;\r\n            &_photo{\r\n                border-radius: 50%;\r\n                width: 4em;\r\n                height: 4em;\r\n                object-fit: cover;\r\n            }\r\n            &_content{\r\n                margin: 0 1em;\r\n            }\r\n        }\r\n    }\r\n\r\n    &_content{\r\n        padding-left: 5.5em;\r\n    }\r\n    &_rep{\r\n        position: absolute;\r\n        right: 1em;\r\n        bottom: 1em;\r\n    }\r\n}\r\n\r\n.menu{\r\n    position: absolute;\r\n    top: 0em;\r\n    right: 1em;\r\n    border-radius: 5px;\r\n    border: solid 3px #26F191;\r\n    padding: 3em 1em 1em 1em;\r\n    width: 150px;\r\n    background: white;\r\n\r\n    &_list{\r\n        &_item{\r\n            margin: 0.5em 0;\r\n            cursor: pointer;\r\n        }\r\n    }\r\n    &_close{\r\n        position: absolute;\r\n        top: 1em;\r\n        right: 1em;\r\n        cursor: pointer;\r\n    }\r\n    &-close{\r\n        visibility: hidden;\r\n    }\r\n    &-extended{\r\n        visibility: visible;\r\n    }\r\n\r\n}\r\n</style>"],"sourceRoot":"src/components"}]}