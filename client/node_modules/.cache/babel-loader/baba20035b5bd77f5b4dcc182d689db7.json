AA;AAAA;AAAA;AACA,gBAAA,MAAA,CAAA,MAAA;;AACA,gBAAA,MAAA,CAAA,WAAA;;AACA,oBAAA,MAAA,CAAA,SAAA,GAAA,CAAA,MAAA,CAAA,MAAA,GAAA,CAAA,IAAA,EAAA,GAAA,CAAA,EAAA;AACA,kBAAA,MAAA,CAAA,aAAA,GAAA,KAAA;AACA;;AACA,gBAAA,MAAA,CAAA,eAAA,GAAA,IAAA;;AANA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAOA,KAfA;AAgBA,IAAA,YAhBA,0BAgBA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,MAAA,CAAA,MAAA;;AACA,gBAAA,MAAA,CAAA,WAAA;;AACA,oBAAA,MAAA,CAAA,MAAA,KAAA,CAAA,EAAA;AACA,kBAAA,MAAA,CAAA,eAAA,GAAA,KAAA;AACA;;AACA,gBAAA,MAAA,CAAA,aAAA,GAAA,IAAA;;AANA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAOA;AAvBA,GAdA;AAuCA,EAAA,OAvCA,qBAuCA;AACA,SAAA,WAAA;AACA;AAzCA,CAAA","sourcesContent":["<template>\r\n  <div id=\"app\">\r\n    <Header />\r\n    <div id=\"recherche\">\r\n      <h1>RECHERCHE</h1>\r\n      <div id=\"tri\">\r\n        <div class=\"input\">\r\n          <input type=\"radio\" id=\"alpha\" name=\"tri\" value=\"alphabetique\" />\r\n          <label for=\"alpha\">Tri alphabétique</label>\r\n        </div>\r\n        <div class=\"input\">\r\n            <input type=\"radio\" id=\"anti_alpha\" name=\"tri\" value=\"nati_alphabetique\"/>\r\n            <label for=\"anti_alpha\">Tri alphabétique</label>\r\n        </div>\r\n\r\n      </div>\r\n    </div>\r\n    <div id=\"cards\">\r\n      <CardAnnuaire\r\n        :email=\"user.email\"\r\n        :key=\"user.email\"\r\n        v-for=\"user in users\"\r\n        class=\"card\"\r\n      />\r\n    </div>\r\n    <div id=\"button\">\r\n      <button @click=\"getLessUsers\" v-if=\"boutonPrecedent\">Précédente</button>\r\n      <button @click=\"getMoreUsers\" v-if=\"boutonSuivant\">Suivante</button>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport Header from \"../components/header\";\r\nimport CardAnnuaire from \"../components/userAnnuaire\";\r\nexport default {\r\n  components: {\r\n    Header,\r\n    CardAnnuaire,\r\n  },\r\n  data() {\r\n    return {\r\n      users: {},\r\n      userCount: 0,\r\n      offset: 0,\r\n      boutonSuivant: true,\r\n      boutonPrecedent: false,\r\n    };\r\n  },\r\n  methods: {\r\n    async getAllUsers() {\r\n      const info = await this.$http.get(\r\n        `/user/users/etudiant?offset=${this.offset}`\r\n      );\r\n      this.users = info.data.rep;\r\n      this.userCount = info.data.count;\r\n    },\r\n    async getMoreUsers() {\r\n      this.offset++;\r\n      this.getAllUsers();\r\n      if (this.userCount - (this.offset + 1) * 10 < 1) {\r\n        this.boutonSuivant = false;\r\n      }\r\n      this.boutonPrecedent = true;\r\n    },\r\n    async getLessUsers() {\r\n      this.offset--;\r\n      this.getAllUsers();\r\n      if (this.offset === 0) {\r\n        this.boutonPrecedent = false;\r\n      }\r\n      this.boutonSuivant = true;\r\n    },\r\n  },\r\n  created() {\r\n    this.getAllUsers();\r\n  },\r\n};\r\n</script>\r\n<style lang=\"scss\" scoped>\r\n$color: #26f191;\r\n#app {\r\n  padding-bottom: 2em;\r\n  user-select: none;\r\n  #recherche {\r\n    height: 40vh;\r\n    display: flex;\r\n    flex-direction: column;\r\n    justify-content: center;\r\n    align-content: center;\r\n    h1 {\r\n      text-align: center;\r\n    }\r\n    #tri {\r\n      display: flex;\r\n      justify-content: center;\r\n      .input {\r\n        margin: 0 2em;\r\n      }\r\n    }\r\n  }\r\n  #cards {\r\n    width: 80vw;\r\n    margin: 0 auto;\r\n    display: flex;\r\n    flex-direction: row;\r\n    justify-content: space-around;\r\n    flex-wrap: wrap;\r\n    .card {\r\n      margin: 1vh 0;\r\n    }\r\n  }\r\n  #button {\r\n    display: flex;\r\n    flex-direction: row;\r\n    justify-content: center;\r\n    margin: 2em 0;\r\n    button {\r\n      background-color: $color;\r\n      color: white;\r\n      border: none;\r\n      border-radius: 5px;\r\n      padding: 0.75em 1em;\r\n      font-weight: 700;\r\n    }\r\n  }\r\n}\r\n</style>"],"sourceRoot":"src/views"}]}