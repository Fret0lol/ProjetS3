;\r\n            }  \r\n        },\r\n        async deleteUser(user) {\r\n            try {\r\n                await this.$http.post(`/user/delete`,user);\r\n                this.getInfoUser();\r\n            } catch (err) {\r\n                console.log(err);\r\n            }\r\n        }\r\n    },\r\n    created() {\r\n        this.getInfoUser();\r\n    },\r\n}\r\n</script>"],"sourceRoot":"src/views"}]}