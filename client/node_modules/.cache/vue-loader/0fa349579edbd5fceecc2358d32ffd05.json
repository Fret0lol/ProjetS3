ass=\"input\">\r\n          <div class=\"inputSex\">\r\n            <input\r\n              type=\"radio\"\r\n              id=\"sexeH\"\r\n              name=\"sexe\"\r\n              value=\"H\"\r\n              v-model=\"register.civilite\"\r\n              required\r\n            />\r\n            <label for=\"sexeH\">Monsieur</label>\r\n          </div>\r\n          <div class=\"inputSex\">\r\n            <input\r\n              type=\"radio\"\r\n              id=\"sexeF\"\r\n              name=\"sexe\"\r\n              value=\"F\"\r\n              v-model=\"register.civilite\"\r\n              required\r\n            />\r\n            <label for=\"sexeF\">Madame</label>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div id=\"prenomEtNom\">\r\n        <div class=\"input\">\r\n          <h5>Prénom</h5>\r\n          <input type=\"text\" id=\"prenom\" placeholder=\"Prénom\" v-model=\"register.prenom\" required />\r\n        </div>\r\n        <div class=\"input\">\r\n          <h5>Nom</h5>\r\n          <input type=\"text\" id=\"nom\" placeholder=\"Nom\" v-model=\"register.nom\" required />\r\n        </div>\r\n      </div>\r\n      \r\n      \r\n      \r\n      <input\r\n        type=\"text\"\r\n        id=\"name\"\r\n        class=\"form-control mb-5\"\r\n        placeholder=\"Nom d'utilisateur\"\r\n        v-model=\"register.nomUtilisateur\"\r\n        required\r\n      />\r\n      <input\r\n        type=\"date\"\r\n        id=\"dateNaissance\"\r\n        class=\"form-control mb-5\"\r\n        v-model=\"register.dateNaissance\"\r\n        required\r\n      />\r\n      <select v-model=\"register.statut\">\r\n        <option disabled value=\"\">Choisissez</option>\r\n        <option>Etudiant</option>\r\n        <option>Professeur</option>\r\n        <option>Entreprise</option>\r\n        <option>Administrateur</option>\r\n      </select>\r\n      <input\r\n        type=\"email\"\r\n        id=\"email\"\r\n        class=\"form-control mb-5\"\r\n        placeholder=\"Email\"\r\n        v-model=\"register.email\"\r\n        required\r\n      />\r\n      <Input type=\"email\" label=\"Email\" placeholder=\"Email\" v-model=\"register.email\" icon=\"at\" />\r\n\r\n      <!-- Password -->\r\n      <input\r\n        type=\"password\"\r\n        id=\"password\"\r\n        class=\"form-control mb-5\"\r\n        placeholder=\"Password\"\r\n        v-model=\"register.password\"\r\n      />\r\n      <p>\r\n        Already have an account??<router-link to=\"/login\"\r\n          >click here</router-link\r\n        >\r\n\r\n        <!-- Sign in button -->\r\n        <center>\r\n          <button class=\"btn btn-primary btn-block w-75 my-4\" type=\"submit\">\r\n            Sign in\r\n          </button>\r\n        </center>\r\n      </p>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport swal from \"sweetalert\";\r\nimport Input from \"@/components/CustomInput\"\r\nexport default {\r\n  components: {\r\n    Input\r\n  },\r\n  data() {\r\n    return {\r\n      register: {\r\n        civilite: \"\",\r\n        prenom: \"\",\r\n        nom: \"\",\r\n        dateNaissance: \"\",\r\n        statut: \"\",\r\n        nomUtilisateur: \"\",\r\n        email: \"\",\r\n        password: \"\",\r\n      },\r\n    };\r\n  },\r\n  methods: {\r\n    async registerUser() {\r\n      try {\r\n        let response = await this.$http.post(\"/user/register\", this.register);\r\n        console.log(response);\r\n        let token = response.data.token;\r\n        if (token) {\r\n          localStorage.setItem(\"jwt\", token);\r\n          this.$router.push(\"/\");\r\n          swal(\"Success\", \"Registration Was successful\", \"success\");\r\n        } else {\r\n          swal(\"Error\", \"Something Went Wrong\", \"Error\");\r\n        }\r\n      } catch (err) {\r\n        let error = err.response;\r\n        if (error.status == 409) {\r\n          swal(\"Error\", error.data.message, \"error\");\r\n        } else {\r\n          swal(\"Error\", error.data.err.message, \"error\");\r\n        }\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style lang=\"scss\">\r\n$color: #26F191;\r\n#app {\r\n  min-width: 400px;\r\n  //width: 100%;\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: center;\r\n  align-items: center;\r\n  margin-top: 3em;\r\n  h3 {\r\n    color: $color;\r\n    font-weight: 700;\r\n    text-align: center;\r\n    margin-bottom: 2em;\r\n  }\r\n  form {\r\n    width: 500px;\r\n    .sex {\r\n      .input {\r\n        display: flex;\r\n        flex-flow: row nowrap;\r\n        justify-content: space-around;\r\n        .inputSex {\r\n\r\n        }\r\n      }\r\n    }\r\n    #prenomEtNom {\r\n      display: flex;\r\n      flex-flow: row nowrap;\r\n      justify-content: space-between;\r\n    }\r\n    input {\r\n      border: 2px solid $color;\r\n      border-radius: 5px;\r\n      &:active,\r\n      &:hover,\r\n      &:focus{\r\n        //border: none;\r\n        outline: none;\r\n      }\r\n    }\r\n  }\r\n}\r\n</style>\r\n"]}]}