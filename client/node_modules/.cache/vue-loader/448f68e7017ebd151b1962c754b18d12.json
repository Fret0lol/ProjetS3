putSex\">\r\n            <input\r\n              type=\"radio\"\r\n              id=\"sexeH\"\r\n              name=\"sexe\"\r\n              value=\"H\"\r\n              v-model=\"register.civilite\"\r\n              required\r\n            />\r\n            <label for=\"sexeH\">Monsieur</label>\r\n          </div>\r\n          <div class=\"inputSex\">\r\n            <input\r\n              type=\"radio\"\r\n              id=\"sexeF\"\r\n              name=\"sexe\"\r\n              value=\"F\"\r\n              v-model=\"register.civilite\"\r\n              required\r\n            />\r\n            <label for=\"sexeF\">Madame</label>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div id=\"prenomEtNom\">\r\n        <div class=\"input\">\r\n          <h5>Prénom</h5>\r\n          <CustomInput type=\"text\" label=\"Prénom\" v-model=\"register.prenom\" icon=\"user\" required/>\r\n        </div>\r\n        <div class=\"input\">\r\n          <h5>Nom</h5>\r\n          <CustomInput type=\"text\" label=\"Nom\" v-model=\"register.nom\" icon=\"user\"/>\r\n        </div>\r\n      </div>\r\n      <input\r\n        type=\"date\"\r\n        id=\"dateNaissance\"\r\n        class=\"date\"\r\n        v-model=\"register.dateNaissance\"\r\n        required\r\n      />\r\n      <CustomInput type=\"text\" label=\"Nom d'utilisateur\" v-model=\"register.nomUtilisateur\" icon=\"user\" required/>\r\n\r\n      \r\n      <CustomInput type=\"email\" label=\"Email\" v-model=\"register.email\" icon=\"at\" required />\r\n      <CustomInput type=\"password\" label=\"Mot de passe\" v-model=\"register.password\" icon=\"lock\" required/>\r\n      <p>\r\n        Déjà un compte ?<router-link to=\"/login\"\r\n          >Se connecter</router-link\r\n        > \r\n      </p>\r\n      <a href=\"\" @click=\"this.$emit('retour', 0)\">Je ne suis pas étudiant</a>\r\n        <!-- Sign in button -->\r\n          <button type=\"submit\">\r\n            S'enregistrer\r\n          </button>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport swal from \"sweetalert\";\r\nimport CustomInput from \"@/components/CustomInput\"\r\nexport default {\r\n  components: {\r\n    CustomInput\r\n  },\r\n  data() {\r\n    return {\r\n      register: {\r\n        civilite: \"\",\r\n        prenom: \"\",\r\n        nom: \"\",\r\n        dateNaissance: \"\",\r\n        statut: \"Etudiant\",\r\n        nomUtilisateur: \"\",\r\n        email: \"\",\r\n        password: \"\",\r\n      },\r\n    };\r\n  },\r\n  methods: {\r\n    async registerUser() {\r\n      try {\r\n        let response = await this.$http.post(\"/user/register\", this.register);\r\n        console.log(response);\r\n        let token = response.data.token;\r\n        if (token) {\r\n          localStorage.setItem(\"jwt\", token);\r\n          this.$router.push(\"/\");\r\n          swal(\"Success\", \"Registration Was successful\", \"success\");\r\n        } else {\r\n          swal(\"Error\", \"Something Went Wrong\", \"Error\");\r\n        }\r\n      } catch (err) {\r\n        let error = err.response;\r\n        if (error.status == 409) {\r\n          swal(\"Error\", error.data.message, \"error\");\r\n        } else {\r\n          swal(\"Error\", error.data.err.message, \"error\");\r\n        }\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style lang=\"scss\" scoped>\r\n$color: #26F191;\r\n#register {\r\n  min-width: 400px;\r\n  //width: 100%;\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: center;\r\n  align-items: center;\r\n  margin-top: 3em;\r\n  form {\r\n    width: 500px;\r\n    .sex {\r\n      .input {\r\n        display: flex;\r\n        flex-flow: row nowrap;\r\n        justify-content: space-around;\r\n      }\r\n    }\r\n    #prenomEtNom {\r\n      display: flex;\r\n      flex-flow: row nowrap;\r\n      justify-content: space-between;\r\n      .input {\r\n        width: 50%;\r\n      }\r\n    }\r\n    button {\r\n      background-color: $color;\r\n      height: 56px;\r\n      font-size: 17px;\r\n      border-radius: 5px;\r\n      border: none;\r\n      color: white;\r\n    }\r\n    .date {\r\n      min-width: 10vw;\r\n      \r\n      padding: 0.9vh 2.2rem 0.9vh 1.2rem;\r\n      outline: none;\r\n      border: 2px solid $color;\r\n      border-radius: 5px;\r\n    }\r\n  }\r\n}\r\n</style>\r\n"]}]}